000104r 2  4A           		lsr
000105r 2  25 rr        		and joy1						; to Down and Right
000107r 2  F0 0A        		beq @GET_PUSHSTART_BTN
000109r 2               		; Use previous frame's directions
000109r 2  A5 rr        		lda joy1
00010Br 2  45 rr        		eor joy1_prev
