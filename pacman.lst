00009Dr 2  A2 00        		ldx #0
00009Fr 2               @SET_MODE:
00009Fr 2  BD 00 06     		lda PPU_UPDATE_DATA, x
0000A2r 2  C9 FF        		cmp #PPU_END_CODE
0000A4r 2  F0 36        		beq @EXIT
0000A6r 2  10 14        		bpl @SET_ADDR					; 0xff~0x7e => @SET_ADDR
0000A8r 2  C9 FC        		cmp #%11111100					; $fc
0000AAr 2  30 10        		bmi @SET_ADDR					; 0xfc~0xff = plus, 0x7c~0xfb => @SET_ADDR
0000ACr 2  29 02        		and #%00000010					; Get mode
0000AEr 2  0A           		asl
