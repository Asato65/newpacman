00012Fr 2               
00012Fr 2               @READ_JOY_LOOP:
00012Fr 2  AD 16 40     		lda JOYPAD1
000132r 2  29 03        		and #%00000011
000134r 2  C9 01        		cmp #$01						; A - 1 = A + 0xff; if A > 0 then Carry=1
000136r 2  26 rr        		rol joy1						; Carry -> bit 0; bit 7 -> Carry
000138r 2  AD 17 40     		lda JOYPAD2
00013Br 2  29 03        		and #%00000011
00013Dr 2  C9 01        		cmp #$01
00013Fr 2  26 rr        		rol joy2
000141r 2  90 EC        		bcc @READ_JOY_LOOP				; CarryON -> end
