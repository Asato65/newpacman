00008Fr 2               .proc NMI
00008Fr 2  08 48 8A 48  		registerSave
000093r 2  98 48        
000095r 2  E6 FD        		inc nmi_cnt
000097r 2  A5 FF        		lda isend_main
000099r 2  F0 41        		beq @EXIT
