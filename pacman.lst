0000BDr 2  BD 00 06     		lda PPU_UPDATE_DATA, x
0000C0r 2  8D 06 20     		sta PPU_ADDR
0000C3r 2  E8           		inx
0000C4r 2  BD 00 06     		lda PPU_UPDATE_DATA, x
0000C7r 2  8D 06 20     		sta PPU_ADDR
0000CAr 2  E8           		inx
